<h1>Add new project</h1>

<%= form_for @project do |f|%>
	<%= f.label :title%>
  <%= f.text_field :title%>
  </br>
  <%= f.label :description%>
  <%= f.text_field :description%>
  </br>
  <%= f.label :abstract%>
  <%= f.text_field :abstract%>
  </br>
  <%= f.label :budget%>
  <%= f.text_field :budget%>
  </br>
  <%= f.submit 'Submit' %>
<%end%>
